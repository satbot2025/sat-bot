# Copy this file to .env.local and fill in values
# Find values in Supabase: Project Settings -> API
NEXT_PUBLIC_SUPABASE_URL=
NEXT_PUBLIC_SUPABASE_ANON_KEY=

# Server-side service role key (never ship to the browser)
SUPABASE_SERVICE_ROLE_KEY=

# Session secret for JWT/cookie signing in Next.js server actions
# Use a long random string (>= 32 chars)
SESSION_SECRET=change-me-please-32+chars

# OpenAI (for /api/chat). Add your real key locally and in Vercel env vars.
# Do NOT prefix with NEXT_PUBLIC_. Keep keys server-only.
OPENAI_API_KEY=sk-your-openai-key
# Optional: choose a model (defaults to gpt-4o-mini)
OPENAI_MODEL=gpt-4o-mini

# Optional: AI chat integration
# OpenAI
OPENAI_API_KEY=
OPENAI_MODEL=gpt-4o-mini

# Gemini
# Get key: https://aistudio.google.com/  (API Keys)
GEMINI_API_KEY=
GEMINI_MODEL=gemini-1.5-flash
