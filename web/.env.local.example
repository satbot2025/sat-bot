# Copy this file to .env.local and fill in values
# Find values in Supabase: Project Settings -> API
NEXT_PUBLIC_SUPABASE_URL=
NEXT_PUBLIC_SUPABASE_ANON_KEY=

# Server-side service role key (never ship to the browser)
SUPABASE_SERVICE_ROLE_KEY=

# Session secret for JWT/cookie signing in Next.js server actions
# Use a long random string (>= 32 chars)
SESSION_SECRET=change-me-please-32+chars

# Optional: AI chat integration
# Get an API key from https://platform.openai.com/
OPENAI_API_KEY=
OPENAI_MODEL=gpt-4o-mini
